{
  "name": "couchers-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/dayjs": "1.x",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "3.3.10",
    "@toast-ui/editor": "^2.5.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.5",
    "google-protobuf": "^3.17.0",
    "grpc-web": "^1.2.1",
    "maplibre-gl": "^1.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^6.15.5",
    "react-intersection-observer": "^8.32.0",
    "react-lines-ellipsis": "^0.15.0",
    "react-query": "^3.16.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "seamless-scroll-polyfill": "^1.2.3",
    "typescript": "~4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/*",
    "lint": "eslint src/",
    "test-ci": "tsc && CI=true react-scripts test --coverageReporters=\"text\" --coverageReporters=\"cobertura\" --coverageReporters=\"lcov\" --reporters=\"default\" --reporters=\"jest-junit\" --coverage",
    "jest": "jest --modulePaths=src",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "NODE_PATH=. build-storybook -s public",
    "typecheck": "tsc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": "warn",
      "simple-import-sort/exports": "warn",
      "sort-imports": "off",
      "import/order": "off",
      "import/first": "error",
      "import/newline-after-import": "error",
      "import/no-duplicates": "error",
      "import/no-relative-parent-imports": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": [
            "warn",
            {
              "argsIgnorePattern": "theme",
              "varsIgnorePattern": "classes|useStyles"
            }
          ]
        }
      }
    ]
  },
  "eslintIgnore": [
    "src/pb/**"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/node-logger": "^6.1.15",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.15",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.1.9",
    "@types/css-mediaquery": "^0.1.1",
    "@types/google-protobuf": "^3.15.2",
    "@types/jest": "^26.0.21",
    "@types/maplibre-gl": "^1.13.1",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webpack-env": "^1.15.3",
    "coveralls": "^3.1.0",
    "css-mediaquery": "^0.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest-junit": "^12.0.0",
    "mockdate": "^3.0.5",
    "msw": "^0.28.2",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/pb/**",
      "!src/stories/**",
      "!src/**/*.stories.tsx"
    ],
    "resetMocks": true
  }
}
