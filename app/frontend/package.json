{
  "name": "couchers-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/mapbox-gl": "<2.0.0",
    "classnames": "^2.2.6",
    "google-protobuf": "^3.13.0",
    "grpc-web": "^1.2.1",
    "mapbox-gl": "<2.0.0",
    "markdown-it": "^12.0.2",
    "moment": "^2.29.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.14.2",
    "react-query": "^3.5.16",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "~4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/",
    "lint": "eslint src/",
    "test-ci": "CI=true react-scripts test --coverageReporters=\"text\" --coverageReporters=\"cobertura\" --coverageReporters=\"lcov\" --reporters=\"default\" --reporters=\"jest-junit\" --coverage",
    "jest": "jest",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "NODE_PATH=. build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": [
            "warn",
            {
              "argsIgnorePattern": "theme",
              "varsIgnorePattern": "classes|useStyles"
            }
          ]
        }
      }
    ]
  },
  "eslintIgnore": [
    "src/pb/**"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/node-logger": "^6.1.2",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/classnames": "^2.2.11",
    "@types/google-protobuf": "^3.7.3",
    "@types/jest": "^26.0.14",
    "@types/markdown-it": "^10.0.3",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack-env": "^1.15.3",
    "coveralls": "^3.1.0",
    "eslint-config-prettier": "^6.12.0",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/pb/**"
    ],
    "resetMocks": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
